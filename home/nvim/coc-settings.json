{
  "sumneko-lua.serverDir": "/home/sean/.local/share/sumneko-lua-language-server/share/lua-language-server",
  "Lua.misc.parameters": [
    "--metapath",
    "~/.cache/sumneko_lua/meta",
    "--logpath",
    "~/.cache/sumneko_lua/log"
  ],
  "Lua.workspace.library": [
    "/home/sean/.config/nvim/lua"
  ],
  "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.runnables.cargoExtraArgs": [
    "--target",
    "thumbv7em-none-eabihf"
  ],
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "languageserver": {
    "nix-nil": {
      "command": "nil",
      "filetypes": [
        "nix"
      ],
      "rootPatterns": [
        "flake.nix"
      ],
      "settings": {
        "nil": {
          "formatting": {
            "command": [
              "nixfmt"
            ]
          }
        }
      }
    },
    "nix-nixd": {
      "command": "nixd",
      "filetypes": ["nix"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": [
        "cmake"
      ],
      "rootPatterns": [
        "build/"
      ],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    }
  },
  "json.schemas": [
    {
      "fileMatch": ["coc-settings.json"],
      "url": "https://raw.githubusercontent.com/neoclide/coc.nvim/release/schema/coc-settings.json"
    }
  ]
}
